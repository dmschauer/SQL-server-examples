      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT &#xA;[Id], &#xA;[AcceptedAnswerId], &#xA;[AnswerCount], &#xA;REPLACE(REPLACE([Body], CHAR(13), ''), CHAR(10), '') AS [Body], &#xA;[ClosedDate], &#xA;[CommentCount], &#xA;[CommunityOwnedDate], &#xA;[CreationDate], &#xA;[FavoriteCount], &#xA;[LastActivityDate], &#xA;[LastEditDate], &#xA;[LastEditorDisplayName], &#xA;[LastEditorUserId], &#xA;[OwnerUserId],&#xA;[ParentId], &#xA;[PostTypeId], &#xA;[Score],&#xA;[Tags],&#xA;[Title], &#xA;[ViewCount]&#xA;FROM dbo.Posts &#xA;ORDER BY id&#xA;OFFSET &quot; + (DT_WSTR, 20) (@[$Package::num_rows_per_file] * @[User::num_files_completed])  + &quot; ROWS&quot; + &quot;&#xA;FETCH NEXT &quot; +  (DT_WSTR, 20) @[$Package::num_rows_per_file] + &quot; ROWS ONLY&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="sql_posts">
      <DTS:VariableValue
        DTS:DataType="8">SELECT 
[Id], 
[AcceptedAnswerId], 
[AnswerCount], 
REPLACE(REPLACE([Body], CHAR(13), ''), CHAR(10), '') AS [Body], 
[ClosedDate], 
[CommentCount], 
[CommunityOwnedDate], 
[CreationDate], 
[FavoriteCount], 
[LastActivityDate], 
[LastEditDate], 
[LastEditorDisplayName], 
[LastEditorUserId], 
[OwnerUserId],
[ParentId], 
[PostTypeId], 
[Score],
[Tags],
[Title], 
[ViewCount]
FROM dbo.Posts 
ORDER BY id
OFFSET 0 ROWS
FETCH NEXT 10 ROWS ONLY</DTS:VariableValue>
    </DTS:Variable>
